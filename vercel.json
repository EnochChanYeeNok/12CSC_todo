{
    "version": 2,
    "builds": [
        {
            "src": "todo.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "todo.py"
        }
    ],
    "functions": {
        "todo.py": {
            "command": "gunicorn -w 4 todo:app"
        }
    }
}