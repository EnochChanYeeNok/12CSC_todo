{
    "version": 2,
    "builds": [
        {
            "src": "todo.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "todo.py"
        }
    ],
    "functions": {
        "todo.py": {
            "runtime": "@vercel/python",
            "timeout": 30
        }
    }

}